<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/briskButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="100dp"
        android:layout_marginBottom="64dp"
        android:text="BRISK"
        app:layout_constraintBottom_toTopOf="@+id/orbButton"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/akazeButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="100dp"
        android:layout_marginBottom="64dp"
        android:text="AKAZE"
        app:layout_constraintBottom_toTopOf="@+id/orbButton"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/kazeButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="100dp"
        android:layout_marginBottom="64dp"
        android:text="KAZE"
        app:layout_constraintBottom_toTopOf="@+id/akazeButton"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/orbButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="64dp"
        android:text="ORB"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/siftButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="100dp"
        android:layout_marginBottom="64dp"
        android:text="SIFT"
        app:layout_constraintBottom_toTopOf="@+id/briskButton"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>